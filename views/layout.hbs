<!DOCTYPE html>
<html>
  <head>
    <title>{{title}}</title>
      <link rel='stylesheet' href='/stylesheets/bootstrap.min.css' />
      <link rel='stylesheet' href='/stylesheets/main.css' />
      <link rel='stylesheet' href='/stylesheets/liteaccordion.css' />
      <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
      <link rel='stylesheet' href='/stylesheets/style.css' />
      <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/gojs/1.8.17/go.js"></script>
      <script type="text/javascript" src="javascripts/jquery.min.js"></script>
      <script type="text/javascript" src="javascripts/bootstrap.bundle.min.js"></script>
      <script type="text/javascript" src="javascripts/bootstrap-confirmation.min.js"></script>
      <script type="text/javascript" src="javascripts/fcm.js"></script>
      <script type="text/javascript" src="javascripts/BalloonLink.js"></script>
      <script type="text/javascript" src="javascripts/jquery.easing.1.3.js"></script>
      <script type="text/javascript" src="javascripts/liteaccordion.jquery.js"></script>
  </head>
  <body>
    {{{body}}}
    <script>
        (function($, d) {
            // demos
            $('#one').liteAccordion({
                autoPlay : false,
                containerWidth: 1542,
                pauseOnHover : true,
                theme : 'stitch',
                rounded : true,
                enumerateSlides : false
            }).find('figcaption:first').show();
            $('#two').liteAccordion();
            $('#three').liteAccordion({ theme : 'dark', rounded : true, enumerateSlides : true, firstSlide : 2, linkable : true, easing: 'easeInOutQuart' });
            $('#four').liteAccordion({ theme : 'light', firstSlide : 3, easing: 'easeOutBounce', activateOn: 'mouseover' });
        })(jQuery, document);

    </script>
  </body>
</html>
